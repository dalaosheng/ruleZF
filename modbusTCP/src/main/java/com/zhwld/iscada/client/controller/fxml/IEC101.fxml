<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.media.*?>
<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import com.zhwld.iscada.controls.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zhwld.iscada.client.controller.IEC101Controller">
   <children>
      <SplitPane orientation="VERTICAL" prefHeight="300.0" prefWidth="600.0">
        <items>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <TableView fx:id="tabBaseRule" depthTest="DISABLE">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <HBox alignment="CENTER" spacing="12.0">
                     <children>
                        <Button fx:id="baseParam" mnemonicParsing="false" text="默认参数" />
                        <Button fx:id="baseConfirm" mnemonicParsing="false" text="保存" />
                        <Button fx:id="IEC101Exit" mnemonicParsing="false" text="退出">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Button>
                     </children>
                     <padding>
                        <Insets top="8.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
        </items>
      </SplitPane>
   </children>
</AnchorPane>
